{
  "name": "react-native-expo-boilerplate",
  "version": "1.0.0",
  "description": "Production-ready React Native Expo boilerplate with zero-setup configuration",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook": "STORYBOOK_ENABLED=true expo start",
    "storybook:generate": "sb-rn-get-stories",
    "generate:component": "node scripts/generate-component.js",
    "generate:screen": "node scripts/generate-screen.js",
    "generate:icons": "node scripts/generate-icons.js",
    "lint": "biome lint .",
    "lint:a11y": "eslint --ext .tsx,.ts src/ --config .eslintrc.js",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "test": "jest",
    "prebuild": "expo prebuild",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "analyze:bundle": "expo export && npx source-map-explorer 'dist/**/*.js' --html bundle-report.html",
    "analyze:size": "size-limit",
    "size": "size-limit",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "screenshots:ios": "node scripts/generate-screenshots.js ios",
    "screenshots:android": "node scripts/generate-screenshots.js android",
    "screenshots:all": "node scripts/generate-screenshots.js all"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.65",
    "@ai-sdk/react": "^2.0.93",
    "@clerk/clerk-expo": "^2.4.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@hookform/resolvers": "^3.9.1",
    "@nozbe/watermelondb": "^0.28.0",
    "@nozbe/with-observables": "^1.6.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.0.12",
    "@sentry/react-native": "^5.33.1",
    "@shopify/flash-list": "^1.7.2",
    "@stripe/stripe-react-native": "^0.57.0",
    "@supabase/supabase-js": "^2.47.10",
    "@tanstack/query-async-storage-persister": "^5.90.11",
    "@tanstack/react-query": "^5.62.7",
    "@tanstack/react-query-persist-client": "^5.90.11",
    "ai": "^5.0.93",
    "axios": "^1.7.9",
